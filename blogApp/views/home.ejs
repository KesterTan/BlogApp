<%-  include("partials/header") -%>
    <h1>Home</h1>
    <p><%= homeStartingContent %></p>
    <% posts.forEach(post => { %>
        <h1>
            <%= post.title %>
        </h1>
        <% const postReadMore = "posts/" + post.title.replace(/\s+/g, '-').toLowerCase(); %>
        <p>
            <%= post.text.slice(0,50) + "... "%><a href=<%= postReadMore%>> Read more</a>
        </p>
    <% }); %> 
<%- include("partials/footer") -%> 

 