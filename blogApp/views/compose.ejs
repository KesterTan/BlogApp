<%-  include("partials/header") -%>
    <h1>Compose</h1>
    <form action="/" method="post">
        <div class="form-group">
            <label for="composeTitle">Title</label>
            <input id ="composeTitle" type="text" class="form-control" name="composeTitle">
          </div>
        <div class="form-group">
            <label for="composeText">Text</label>
            <textarea class="form-control" id="composeText" name="composeText" rows="6"></textarea>
        </div>
        <button class="btn btn-primary" type="submit" name ="submit" autocomplete="off">Publish</button>
    </form>
<%- include("partials/footer") -%> 